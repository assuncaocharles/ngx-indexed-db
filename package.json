{
	"name": "ngx-indexed-db",
	"author": "Charles Assunção",
	"description": "Angular wrapper to IndexedDB database.",
	"version": "5.0.4",
	"license": "ISC",
	"files": [
		"dist/**/*"
	],
	"main": "./dist/ngx-indexed-db/bundles/ngx-indexed-db.umd.js",
	"engines": {
		"node": ">= 10.16.0"
	},
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build --project ngx-indexed-db --prod",
		"typedoc": "typedoc --out ./docs --mode modules --tsconfig ./projects/ngx-indexed-db/tsconfig.lib.json ",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"format": "prettier \"projects/**/*.{ts,json}\" --write"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run format",
			"pre-push": "npm run lint"
		}
	},
	"private": false,
	"keywords": [
		"indexed",
		"db",
		"indexeddb",
		"angular",
		"ng",
		"ngx",
		"storage"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/assuncaocharles/ngx-indexed-db"
	},
	"dependencies": {
		"@angular/common": "^9.1.0",
		"@angular/compiler": "^9.1.0",
		"@angular/core": "^9.1.0",
		"@angular/platform-browser-dynamic": "^9.1.0",
		"nan": "^2.14.0",
		"rxjs": "^6.5.4",
		"zone.js": "^0.10.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.901.0",
		"@angular-devkit/build-ng-packagr": "~0.901.0",
		"@angular/cli": "^9.0.3",
		"@angular/compiler-cli": "^9.1.0",
		"@angular/language-service": "^9.1.0",
		"@types/jasmine": "^3.4.0",
		"@types/jasminewd2": "^2.0.6",
		"@types/node": "^13.1.0",
		"codelyzer": "^5.0.1",
		"cz-conventional-changelog": "^3.0.2",
		"husky": "^4.0.2",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "^5.0.2",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "^3.0.0",
		"karma-jasmine": "~2.0.0",
		"karma-jasmine-html-reporter": "^1.4.2",
		"lodash": "^4.17.15",
		"marked": "^0.8.0",
		"ng-packagr": "^9.1.0",
		"prettier": "^1.18.2",
		"protractor": "~6.0.0",
		"shx": "^0.3.2",
		"tar": "^6.0.2",
		"ts-node": "^8.3.0",
		"tsickle": "^0.38.1",
		"tslib": "^1.10.0",
		"tslint": "~5.17.0",
		"typedoc": "^0.16.0",
		"typescript": "^3.8.3",
		"webpack-dev-server": "^3.8.0"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
